<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <layer-list>
            <item android:left="-1dp">
                <shape>
                    <stroke android:width="@dimen/lay_1" android:color="@color/C_FFFFFF" />
                    <solid android:color="@color/C_5777A6" />
                    <corners android:bottomLeftRadius="0dp" android:bottomRightRadius="4dp" android:topLeftRadius="0dp" android:topRightRadius="4dp" />
                </shape>
            </item>

        </layer-list>
    </item>
    <item android:state_enabled="false">
        <layer-list>
            <item android:left="-1dp">
                <shape>
                    <stroke android:width="@dimen/lay_1" android:color="@color/C_FFFFFF" />
                    <solid android:color="@color/C_FFFFFF" />
                    <corners android:bottomLeftRadius="0dp" android:bottomRightRadius="4dp" android:topLeftRadius="0dp" android:topRightRadius="4dp" />
                </shape>
            </item>

        </layer-list>
    </item>
</selector>
